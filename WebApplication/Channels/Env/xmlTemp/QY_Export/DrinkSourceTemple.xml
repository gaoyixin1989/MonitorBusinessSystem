<?xml version="1.0" encoding="utf-8" ?>

<!--饮用水源地导出配置信息-->

<!--模板设置-->
<GlobalSettings type="export">
  <!--设置监测监测点-->
  <Point pointtable="T_ENV_P_DRINK_SRC" verticaltable="T_ENV_P_DRINK_SRC_V" itemtable="T_ENV_P_DRINK_SRC_V_ITEM">
    <PointInfo>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="5"  column="3">七星岗</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">左</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="6"  column="3">七星岗</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">中</Vertical>
