        <SubPointCode tablecolumn="SECTION_CODE">XK010</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="7"  column="1">里峪</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK009</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="9"  column="1">四道河</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK008</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="11"  column="1">大坝</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
	  <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK014</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="13"  column="1">东入口</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
	  <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK012</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="15"  column="1">西入口</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
	  <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK011</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="17"  column="1">出口</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
	  <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">XK013</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="19"  column="1">桃林口水库</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
    </PointInfo>
  </Point>
  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_LAKE</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>SECTION_ID</FillTablePointColumnName>
      <!--数据填报表对应的垂线字段名称【非必须】-->
      <FillTableVerticalColumnName>POINT_ID</FillTableVerticalColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_LAKE_ITEM</FillItemTableName>
    </Config>
    <FillInfo>
      <FillData tablecolumn="YEAR" column="3">年</FillData>
      <FillData tablecolumn="MONTH" column="4">月</FillData>
      <FillData tablecolumn="DAY" column="5">日</FillData>
      <FillData tablecolumn="JUDGE" column="37">总评价</FillData>
