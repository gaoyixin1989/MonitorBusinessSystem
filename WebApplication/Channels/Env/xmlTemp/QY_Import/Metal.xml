<?xml version="1.0" encoding="utf-8" ?>

<!--底泥重金属导入配置信息-->

<!--模板设置-->
<GlobalSettings type="import">
  <!--设置监测监测点-->
  <Point pointtable="T_ENV_P_SEDIMENT"  itemtable="T_ENV_P_SEDIMENT_ITEM" startrow="5" endrow="9">
    <PointInfo>
      <Column tablecolumn="POINT_CODE" column="3">河流名称</Column>
      <Column tablecolumn="POINT_NAME" column="4" primary="true">断面名称</Column>
    </PointInfo>
  </Point>
  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_SEDIMENT</FillTableName>
      <!--数据填报表对应的监测河流断面名称【必须】-->
      <FillTablePointColumnName>POINT_ID</FillTablePointColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_SEDIMENT_ITEM</FillItemTableName>
      <!--数据填报表序列号名称【必须】-->
      <FillTableSerialNum>sediment_fill_id</FillTableSerialNum>
      <!--数据填报监测项目表序列号名称【必须】-->
      <FillItemTableSerialNum>sediment_fill_item_id</FillItemTableSerialNum>
    </Config>
    <FillInfo>
      <FillData tablecolumn="YEAR" column="0">年</FillData>
      <FillData tablecolumn="MONTH" column="1">月</FillData>
      <FillData tablecolumn="DAY" column="2">日</FillData>
    </FillInfo>
    <ItemInfo tablecolumn="ITEM_VALUE">
      <Item column="5">铜</Item>
      <Item column="6">锌</Item>
      <Item column="7">硒</Item>
      <Item column="8">砷</Item>
      <Item column="9">汞</Item>
      <Item column="10">镉</Item>
      <Item column="11">六价铬</Item>
      <Item column="12">铅</Item>
      <Item column="13">铁</Item>
      <Item column="14">锰</Item>
      <Item column="15">锑</Item>
      <Item column="16">镍</Item>
      <Item column="17">钒</Item>
      <Item column="18">铊</Item>
      <Item column="19">钴</Item>
      <Item column="20">钼</Item>
      <Item column="21">铍</Item>
      <Item column="22">钡</Item>
      <Item column="23">硼</Item>
      <Item column="24">钛</Item>
    </ItemInfo>
  </FillData>
</GlobalSettings>