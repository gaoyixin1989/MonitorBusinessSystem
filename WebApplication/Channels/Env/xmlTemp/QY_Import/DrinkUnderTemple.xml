<?xml version="1.0" encoding="utf-8" ?>

<!--湖库导出配置信息-->

<!--模板设置-->
<GlobalSettings type="import">
  <!--设置监测监测点-->
  <Point pointtable="T_ENV_P_DRINK_UNDER" itemtable="T_ENV_P_DRINK_UNDER_ITEM" startrow="2" endrow="4">
    <PointInfo>
      <SubPoint>
        <SubPointName tablecolumn="POINT_NAME" row="2"  column="1">银盏水库</SubPointName>
      </SubPoint>
	  <SubPoint>
        <SubPointName tablecolumn="POINT_NAME" row="3"  column="1">迎咀水库</SubPointName>
      </SubPoint>
    </PointInfo>
  </Point>
  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_DRINK_UNDER</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>POINT_ID</FillTablePointColumnName>
      <!--数据填报表对应的垂线字段名称【非必须】-->
      <FillTableVerticalColumnName></FillTableVerticalColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_DRINK_UNDER_ITEM</FillItemTableName>
      <!--数据填报表序列号名称【必须】-->
      <FillTableSerialNum>drink_under_fill_id</FillTableSerialNum>
      <!--数据填报监测项目表序列号名称【必须】-->
      <FillItemTableSerialNum>drink_under_fill_item_id</FillItemTableSerialNum>
    </Config>
    <FillInfo>
      <FillData tablecolumn="YEAR" column="0">年</FillData>
      <FillData tablecolumn="MONTH" column="2">月</FillData>
      <FillData tablecolumn="DAY" column="3">日</FillData>
    </FillInfo>
    <ItemInfo tablecolumn="ITEM_VALUE">
		<Item column="4">透明度</Item>
    <Item column="5">PH值</Item>
		<Item column="6">悬浮物</Item>
		<Item column="7">电导率</Item>
		<Item column="8">砷</Item>
		<Item column="9">铜</Item>
		<Item column="10">铅</Item>
		<Item column="11">锌</Item>
		<Item column="12">镉</Item>
		<Item column="13">六价铬</Item>
		<Item column="14">汞</Item>
		<Item column="15">溶解氧</Item>
		<Item column="16">氨氮</Item>
		<Item column="17">总氮</Item>
		<Item column="18">总磷</Item>
		<Item column="19">氟化物</Item>
		<Item column="20">氰化物</Item>
		<Item column="21">硫化物</Item>
		<Item column="22">硒</Item>
		<Item column="23">化学需氧量</Item>
		<Item column="24">五日生化需氧量</Item>
		<Item column="25">挥发酚</Item>
