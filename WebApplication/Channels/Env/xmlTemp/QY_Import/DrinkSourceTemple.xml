<!--饮用水源地导入配置信息-->

<!--模板设置-->
<GlobalSettings type="import">
  <!--设置监测监测点-->
  <Point pointtable="T_ENV_P_DRINK_SRC" verticaltable="T_ENV_P_DRINK_SRC_V" itemtable="T_ENV_P_DRINK_SRC_V_ITEM">
    <PointInfo>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="5"  column="3">七星岗</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">左</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="6"  column="3">七星岗</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">中</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="7"  column="3">七星岗</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">右</Vertical>
      </SubPoint>
    </PointInfo>
  </Point>
  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_DRINK_SRC</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>SECTION_ID</FillTablePointColumnName>
      <!--数据填报表对应的垂线字段名称【非必须】-->
      <FillTableVerticalColumnName>POINT_ID</FillTableVerticalColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_DRINK_SRC_ITEM</FillItemTableName>
      <!--数据填报表序列号名称【必须】-->
      <FillTableSerialNum>drink_src_fill_id</FillTableSerialNum>
      <!--数据填报监测项目表序列号名称【必须】-->
      <FillItemTableSerialNum>drink_src_fill_item_id</FillItemTableSerialNum>
    </Config>
    <FillInfo>
      <FillData tablecolumn="YEAR" column="0">年</FillData>
      <FillData tablecolumn="MONTH" column="1">月</FillData>
      <FillData tablecolumn="DAY" column="2">日</FillData>
