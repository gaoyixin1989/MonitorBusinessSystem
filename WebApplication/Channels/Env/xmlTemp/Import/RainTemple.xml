<?xml version="1.0" encoding="utf-8" ?>
<!--降水导入配置信息-->
<!--模板设置-->
<GlobalSettings type="import">
  <!--pointtable:降水；itemtable:降水监测项目；startrow:Excel的开始行；endrow:Excel的结束行号-->
  <Point pointtable="T_ENV_P_RAIN" itemtable="T_ENV_P_RAIN_ITEM" startrow="1" endrow="200">
    <PointInfo>
      <Column tablecolumn="POINT_CODE" primary="true"  column="1">监测点代码</Column> 
    </PointInfo>
  </Point>

  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_RAIN</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>POINT_ID</FillTablePointColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_RAIN_ITEM</FillItemTableName>
      <!--数据填报表序列号名称【必须】-->
      <FillTableSerialNum>rain_fill_id</FillTableSerialNum>
      <!--数据填报监测项目表序列号名称【必须】-->
      <FillItemTableSerialNum>rain_fill_item_id</FillItemTableSerialNum>
    </Config>

    <!--数据填报表字段-->
    <FillInfo>
      <FillData tablecolumn="YEAR"   column="0">年</FillData>
      <FillData tablecolumn="MONTH"  column="2">月</FillData>
      <FillData tablecolumn="BEGIN_MONTH"  column="3">开始月</FillData>
      <FillData tablecolumn="BEGIN_DAY" primary="true"  column="4">开始日</FillData> 
      <FillData tablecolumn="BEGIN_HOUR"  column="5">开始时</FillData>
      <FillData tablecolumn="BEGIN_MINUTE"  column="6">开始分</FillData>
      <FillData tablecolumn="END_MONTH"  column="7">结束月</FillData>
      <FillData tablecolumn="END_DAY"  column="8">结束日</FillData>
      <FillData tablecolumn="END_HOUR"   column="9">结束时</FillData>
      <FillData tablecolumn="END_MINUTE"  column="10">结束分</FillData>
      <FillData tablecolumn="RAIN_TYPE"  column="11">降水类型</FillData>
    </FillInfo>

    <!--数据填报监测项目表字段-->
    <ItemInfo tablecolumn="ITEM_VALUE">
      <Item column="12">降雨量</Item>
      <Item column="13">PH值</Item>
      <Item column="14">电导率</Item>
      <Item column="15">硫酸盐</Item>
      <Item column="16">硝酸盐</Item>
      <Item column="17">氟离子</Item>
      <Item column="18">氯离子</Item>
      <Item column="19">氨离子</Item>
      <Item column="20">钙</Item>
      <Item column="21">镁</Item>
      <Item column="22">钠</Item>
      <Item column="23">钾</Item>
    </ItemInfo>

  </FillData>



</GlobalSettings>