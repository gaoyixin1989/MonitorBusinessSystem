    <PointInfo>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">01</SubPointCode>
        <SubPointName tablecolumn="SECTION_NAME" row="4"  column="6">二水厂</SubPointName>
        <Vertical pointcolumn="SECTION_ID" tablecolumn="VERTICAL_NAME">监测点</Vertical>
      </SubPoint>
      <SubPoint>
        <SubPointCode tablecolumn="SECTION_CODE">02</SubPointCode>
