<?xml version="1.0" encoding="utf-8" ?>

<!--地下饮用水导入配置信息-->

<!--模板设置-->
<GlobalSettings type="import">
  <!--设置监测监测点-->
  <Point pointtable="T_ENV_P_DRINK_UNDER" verticaltable="" itemtable="T_ENV_P_DRINK_UNDER_ITEM">
    <PointInfo>
      <SubPoint>
        <SubPointCode tablecolumn="POINT_CODE">XD032</SubPointCode>
        <SubPointName tablecolumn="POINT_NAME" row="11"  column="0">柳江</SubPointName>
      </SubPoint>
	  <SubPoint>
        <SubPointCode tablecolumn="POINT_CODE">XD031</SubPointCode>
        <SubPointName tablecolumn="POINT_NAME" row="13"  column="0">枣园</SubPointName>
      </SubPoint>
    </PointInfo>
  </Point>
  <!--数据填报信息-->
  <FillData>
    <Config>
      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_DRINK_UNDER</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>POINT_ID</FillTablePointColumnName>
	  <!--数据填报表对应的垂线字段名称【非必须】-->
      <FillTableVerticalColumnName></FillTableVerticalColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_DRINK_UNDER_ITEM</FillItemTableName>
      <!--数据填报表序列号名称【必须】-->
      <FillTableSerialNum>drink_under_fill_id</FillTableSerialNum>
      <!--数据填报监测项目表序列号名称【必须】-->
      <FillItemTableSerialNum>drink_under_fill_item_id</FillItemTableSerialNum>
    </Config>
    <FillInfo>
      <FillData tablecolumn="YEAR" column="2">年</FillData>
      <FillData tablecolumn="MONTH" column="3">月</FillData>
      <FillData tablecolumn="DAY" column="4">日</FillData>
    </FillInfo>
    <ItemInfo tablecolumn="ITEM_VALUE">
      <Item column="5">水温</Item>
      <Item column="6">PH值</Item>
	  <Item column="7">总硬度</Item>
	  <Item column="8">氯化物</Item>
