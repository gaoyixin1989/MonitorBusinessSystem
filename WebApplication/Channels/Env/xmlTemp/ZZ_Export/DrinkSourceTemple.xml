      <!--数据填报表名称【必须】-->
      <FillTableName>T_ENV_FILL_DRINK_SRC</FillTableName>
      <!--数据填报表对应的监测点字段名称【必须】-->
      <FillTablePointColumnName>SECTION_ID</FillTablePointColumnName>
      <!--数据填报表对应的垂线字段名称【非必须】-->
      <FillTableVerticalColumnName>POINT_ID</FillTableVerticalColumnName>
      <!--数据填报监测项目表名称【必须】-->
      <FillItemTableName>T_ENV_FILL_DRINK_SRC_ITEM</FillItemTableName>
    </Config>
    <FillInfo>
