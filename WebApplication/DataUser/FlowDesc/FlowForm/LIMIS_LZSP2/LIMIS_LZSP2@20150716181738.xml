    <Tag>use [EMBMS_MAS]

select task_id = DBO.T_MIS_MONITOR_TASK.TICKET_NUM, sample_name=DBO.T_MIS_MONITOR_TASK.PROJECT_NAME, 
analysis_item=dbo.T_BASE_ITEM_INFO.ITEM_NAME, sample_date = dbo.T_MIS_MONITOR_SUBTASK.SAMPLE_FINISH_DATE,
analysis_date=dbo.T_MIS_MONITOR_RESULT_APP.FINISH_DATE, sample_store=dbo.T_MIS_MONITOR_RESULT.REMARK_2,
item_standard = dbo.T_BASE_METHOD_INFO.METHOD_CODE + ~ ~ +dbo.T_BASE_METHOD_INFO.METHOD_NAME,
equip_type_no = dbo.T_BASE_APPARATUS_INFO.NAME + ~ ~ + dbo.T_BASE_APPARATUS_INFO.MODEL
from 
dbo.T_MIS_MONITOR_RESULT
left join dbo.T_MIS_MONITOR_RESULT_APP on (dbo.T_MIS_MONITOR_RESULT_APP.RESULT_ID = dbo.T_MIS_MONITOR_RESULT.ID)
left join dbo.T_MIS_MONITOR_SAMPLE_INFO on (dbo.T_MIS_MONITOR_RESULT.SAMPLE_ID = dbo.T_MIS_MONITOR_SAMPLE_INFO.ID)
left join dbo.T_MIS_MONITOR_SUBTASK on (T_MIS_MONITOR_SAMPLE_INFO.SUBTASK_ID = dbo.T_MIS_MONITOR_SUBTASK.ID)
left join dbo.T_MIS_MONITOR_TASK on (dbo.T_MIS_MONITOR_TASK.ID = dbo.T_MIS_MONITOR_SUBTASK.task_ID)

left join dbo.T_BASE_METHOD_INFO on (dbo.T_MIS_MONITOR_RESULT.STANDARD_ID= dbo.T_BASE_METHOD_INFO.ID)
left join T_BASE_ITEM_INFO on (dbo.T_MIS_MONITOR_RESULT.ITEM_ID = dbo.T_BASE_ITEM_INFO.ID)

left join dbo.T_BASE_ITEM_ANALYSIS on (dbo.T_BASE_ITEM_INFO.ID = dbo.T_BASE_ITEM_ANALYSIS.ITEM_ID and dbo.T_BASE_ITEM_ANALYSIS.ID = dbo.T_MIS_MONITOR_RESULT.ANALYSIS_METHOD_ID)
left join dbo.T_BASE_APPARATUS_INFO on (dbo.T_BASE_APPARATUS_INFO.ID = dbo.T_BASE_ITEM_ANALYSIS.INSTRUMENT_ID)

where dbo.T_MIS_MONITOR_RESULT.CCFLOW_ID1 = @OID
</Tag>
    <Tag1>*DtlLIMIS_LZSP2334320=use [EMBMS_MAS]

select sample_no=dbo.T_MIS_MONITOR_SAMPLE_INFO.SAMPLE_NAME, analysis_item=dbo.T_BASE_ITEM_INFO.ITEM_NAME
from T_BASE_ITEM_INFO
left join dbo.T_MIS_MONITOR_RESULT on (dbo.T_MIS_MONITOR_RESULT.ITEM_ID = dbo.T_BASE_ITEM_INFO.ID)
left join dbo.T_MIS_MONITOR_SAMPLE_INFO on (dbo.T_MIS_MONITOR_RESULT.SAMPLE_ID = dbo.T_MIS_MONITOR_SAMPLE_INFO.ID)
left join dbo.T_MIS_MONITOR_SUBTASK on (T_MIS_MONITOR_SAMPLE_INFO.SUBTASK_ID = dbo.T_MIS_MONITOR_SUBTASK.ID)
left join dbo.T_MIS_MONITOR_TASK on (dbo.T_MIS_MONITOR_TASK.ID = dbo.T_MIS_MONITOR_SUBTASK.task_ID)
where dbo.T_MIS_MONITOR_RESULT.CCFLOW_ID1 = @OID*DtlLIMIS_LZSP2361601=use [EMBMS_MAS]

select sample_id = c.sample_name, result1=A.TWIN_RESULT1, result2=A.TWIN_RESULT2, result=A.TWIN_AVG, gap=A.TWIN_OFFSET,

satisfied=
case TWIN_ISOK
when 0 then ~是~
else ~否~
end

from dbo.T_MIS_MONITOR_QC_TWIN as A
left join dbo.T_MIS_MONITOR_RESULT as B on (B.ID = A.RESULT_ID_SRC)
left join dbo.T_MIS_MONITOR_SAMPLE_INFO as C on (B.SAMPLE_ID = C.ID)
left join dbo.T_MIS_MONITOR_SUBTASK on (C.SUBTASK_ID = dbo.T_MIS_MONITOR_SUBTASK.ID)
left join dbo.T_MIS_MONITOR_TASK on (dbo.T_MIS_MONITOR_TASK.ID = dbo.T_MIS_MONITOR_SUBTASK.task_ID)

where B.CCFLOW_ID1 = @OID
